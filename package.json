{
  "name": "bouton",
  "version": "0.7.2",
  "description": "A framework to build your reactive library",
  "main": "lib/index.js",
  "dependencies": {
    "eventemitter3": "^1.2.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-flow": "^7.18.6",
    "babel-loader": "^9.1.0",
    "babel-plugin-conditional-compilation": "0.0.1",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "flow-bin": "^0.27.0",
    "nodeunit": "^0.9.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "scripts": {
    "test": "node_modules/nodeunit/bin/nodeunit test",
    "flow": "babel ./src --out-dir ./flow-output",
    "flow-watch": "babel -w ./src --out-dir ./flow-output",
    "build": "babel ./flow-output --out-dir ./lib",
    "build-watch": "babel -w ./flow-output --out-dir ./lib",
    "webpack": "webpack",
    "webpack-watch": "webpack -w",
    "webpack-prod": "export NODE_ENV=production && webpack --output-filename bouton.min.js",
    "webpack-compatible": "export NODE_ENV=production && webpack --output-filename bouton.compatible.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhou/bouton.js.git"
  },
  "keywords": [
    "reactive",
    "programming"
  ],
  "author": "Bo HOU <daily.bhou@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bhou/bouton.js/issues"
  },
  "homepage": "https://github.com/bhou/bouton.js#readme"
}
